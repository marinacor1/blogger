<h1> All Articles </h1>

<ul id="articles">
  <% @articles.each do |article| %>
  <li>
    <%= link_to article.title, article_path(article), class: 'article_title'%>
  </li>
  <% end %>
</ul>
<%= link_to "Create New Article", new_article_path %>
<!--  article_path(1) is the same thing as /articles/1-->
